{
  "name": "bookmark-finder-extension-ui",
  "version": "1.0.17",
  "description": "Bookmark Finder UI Components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/turtle601/bookmark-finder-extension.git",
    "directory": "packages/ui"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "bookmark-finder-extension-eslint-config": "workspace:*",
    "bookmark-finder-extension-prettier-config": "workspace:*",
    "@chromatic-com/storybook": "^3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/react-vite": "^9.1.6",
    "@svgr/rollup": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "rollup": "^4.36.0",
    "rollup-plugin-dts": "^6.1.1",
    "storybook": "^9.1.6",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "scripts": {
    "lint::check": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint::fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier::check": "prettier src/**/* --check",
    "prettier::fix": "prettier src/**/* --write --cache",
    "clean": "rm -rf dist",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prettier": "bookmark-finder-extension-prettier-config"
}
