{
  "name": "bookmark-finder-extension",
  "version": "3.0.3",
  "description": "Bookmark Finder Chrome Extension",
  "author": "Hwang Jun Seung, turtle601",
  "keywords": [
    "bookmark",
    "extension",
    "chrome"
  ],
  "scripts": {
    "prebuild": "npx rimraf dist",
    "build": "webpack build --config ./webpack.config.js --mode production",
    "lint::check": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint::fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier::check": "prettier src/**/* --check",
    "prettier::fix": "prettier src/**/* --write --cache",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@google/generative-ai": "^0.14.1",
    "@tanstack/react-query": "^5.51.1",
    "@tanstack/react-query-devtools": "^5.51.1",
    "dotenv": "^16.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-shadow": "^20.5.0",
    "zustand": "^4.5.2",
    "bookmark-finder-extension/ui": "workspace:*"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.51.1",
    "bookmark-finder-extension/eslint-config": "workspace:*",
    "bookmark-finder-extension/prettier-config": "workspace:*",
    "@types/chrome": "^0.0.268",
    "babel-loader": "^9.1.3",
    "chromatic": "^11.7.1",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-loader": "^9.5.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "prettier": "bookmark-finder-extension/prettier-config"
}
